{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "server": {
    "tenant": {
      "mode": "StaticSingle",
      "tenantPath": "/server",
      "staticSingle": {
        "tenantConfiguration": {
          "tenantId": "foo",
          "displayName": "Foo Tenant"
        }
      }
    }
  },
  "settings": {
    "scopes_supported": [
      "openid",
      "profile",
      "email",
      "address",
      "phone",
      "offline_access"
    ],
    "response_types_supported": [
      "none",
      "code",
      "id_token",
      "token"
    ]
  }
}
